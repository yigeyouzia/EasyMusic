	</insert>

	<!--多条件修改-->
	<update id="updateByParam" parameterType="com.easymusic.entity.query.PayCodeInfoQuery">
		 UPDATE pay_code_info p
 		 <set> 
			<if test="bean.payCode != null">
				 pay_code = #{bean.payCode},
			</if>
			<if test="bean.amount != null">
				 amount = #{bean.amount},
			</if>
			<if test="bean.createTime != null">
				 create_time = #{bean.createTime},
			</if>
			<if test="bean.useUserId != null">
				 use_user_id = #{bean.useUserId},
			</if>
			<if test="bean.useTime != null">
				 use_time = #{bean.useTime},
			</if>
			<if test="bean.status != null">
				 status = #{bean.status},
			</if>
 		 </set>
 		 <include refid="query_condition" />
	</update>

	<!--多条件删除-->
	<delete id="deleteByParam">
		 delete p from pay_code_info p
 		 <include refid="query_condition" />
	</delete>

	<!-- 根据PayCode修改-->
	<update id="updateByPayCode" parameterType="com.easymusic.entity.po.PayCodeInfo">
		 UPDATE pay_code_info
 		 <set> 
			<if test="bean.amount != null">
				 amount = #{bean.amount},
			</if>
			<if test="bean.createTime != null">
				 create_time = #{bean.createTime},
			</if>
			<if test="bean.useUserId != null">
				 use_user_id = #{bean.useUserId},
			</if>
			<if test="bean.useTime != null">
				 use_time = #{bean.useTime},
			</if>
			<if test="bean.status != null">
				 status = #{bean.status},
			</if>
 		 </set>
 		 where pay_code=#{payCode}
	</update>

	<!-- 根据PayCode删除-->
	<delete id="deleteByPayCode">
		delete from pay_code_info where pay_code=#{payCode}
	</delete>

	<!-- 根据PrimaryKey获取对象-->
	<select id="selectByPayCode" resultMap="base_result_map" >
		select <include refid="base_column_list" /> from pay_code_info p where pay_code=#{payCode}
	</select>

</mapper>