			</if>
			<if test="bean.status!=null">
				 status = VALUES(status),
			</if>
		 </trim>
	</insert>

	<!-- 添加 （批量插入）-->
	<insert id="insertBatch" parameterType="com.easymusic.entity.po.PayCodeInfo">
		 INSERT INTO pay_code_info(
			 pay_code,
			 amount,
			 create_time,
			 use_user_id,
			 use_time,
			 status
		 )values
		 <foreach collection="list" item="item" separator=",">
			 (
