<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.easymusic.mappers.PayCodeInfoMapper">

	<!--实体映射-->
	<resultMap id="base_result_map" type="com.easymusic.entity.po.PayCodeInfo">
		<!--支付码-->
		<result column="pay_code" property="payCode"  />
		<!--金额-->
		<result column="amount" property="amount"  />
		<!--创建时间-->
		<result column="create_time" property="createTime"  />
		<!--使用用户ID-->
		<result column="use_user_id" property="useUserId"  />
		<!--使用时间-->
		<result column="use_time" property="useTime"  />
		<!--状态 0:待使用 1:已使用-->
		<result column="status" property="status"  />
	</resultMap>


	<!-- 通用查询结果列-->
	<sql id="base_column_list">
		 p.pay_code,p.amount,p.create_time,p.use_user_id,p.use_time,
		 p.status
	</sql>

	<sql id="base_condition_filed">
			<if test="query.payCode != null and query.payCode!=''">
				 and  p.pay_code = #{query.payCode}
			</if>
			<if test="query.amount != null">
				 and  p.amount = #{query.amount}
			</if>
			<if test="query.createTime != null and query.createTime!=''">
				 <![CDATA[ and  p.create_time=str_to_date(#{query.createTime}, '%Y-%m-%d') ]]>
			</if>
			<if test="query.useUserId != null and query.useUserId!=''">
				 and  p.use_user_id = #{query.useUserId}
			</if>
			<if test="query.useTime != null and query.useTime!=''">
				 <![CDATA[ and  p.use_time=str_to_date(#{query.useTime}, '%Y-%m-%d') ]]>
			</if>
			<if test="query.status != null">
				 and  p.status = #{query.status}
			</if>
	</sql>

	<!-- 通用查询条件列-->
	<sql id="query_condition">
	 <where>
			<include refid="base_condition_filed" />
			<if test="query.payCodeFuzzy!= null  and query.payCodeFuzzy!=''">
				 and  p.pay_code like concat('%', #{query.payCodeFuzzy}, '%')
			</if>
			<if test="query.createTimeStart!= null and query.createTimeStart!=''">
				 <![CDATA[ and  p.create_time>=str_to_date(#{query.createTimeStart}, '%Y-%m-%d') ]]>
			</if>
			<if test="query.createTimeEnd!= null and query.createTimeEnd!=''">
				 <![CDATA[ and  p.create_time< date_sub(str_to_date(#{query.createTimeEnd},'%Y-%m-%d'),interval -1 day) ]]>
			</if>
			<if test="query.useUserIdFuzzy!= null  and query.useUserIdFuzzy!=''">
				 and  p.use_user_id like concat('%', #{query.useUserIdFuzzy}, '%')
			</if>
			<if test="query.useTimeStart!= null and query.useTimeStart!=''">
				 <![CDATA[ and  p.use_time>=str_to_date(#{query.useTimeStart}, '%Y-%m-%d') ]]>
			</if>
			<if test="query.useTimeEnd!= null and query.useTimeEnd!=''">
				 <![CDATA[ and  p.use_time< date_sub(str_to_date(#{query.useTimeEnd},'%Y-%m-%d'),interval -1 day) ]]>
			</if>
	 </where>
	</sql>

	<!-- 查询集合-->
	<select id="selectList" resultMap="base_result_map" >
		SELECT <include refid="base_column_list" />
		<if test="query.queryUser">
			,u.nick_name nickName
		</if>
		FROM pay_code_info p
		<include refid="query_condition" />
		<if test="query.queryUser">
			left join user_info u on p.use_user_id = u.user_id
		</if>
		 <if test="query.orderBy!=null">
			 order by ${query.orderBy}
		 </if>
		 <if test="query.simplePage!=null">
			 limit #{query.simplePage.start},#{query.simplePage.end}
		 </if>
	</select>

	<!-- 查询数量-->
	<select id="selectCount" resultType="java.lang.Integer" >
		 SELECT count(1) FROM pay_code_info p <include refid="query_condition" />
	</select>

	<!-- 插入 （匹配有值的字段）-->
	<insert id="insert" parameterType="com.easymusic.entity.po.PayCodeInfo">
		 INSERT INTO pay_code_info
		 <trim prefix="(" suffix=")" suffixOverrides="," >
			<if test="bean.payCode != null">
				 pay_code,
			</if>
			<if test="bean.amount != null">
				 amount,
			</if>
			<if test="bean.createTime != null">
				 create_time,
			</if>
			<if test="bean.useUserId != null">
				 use_user_id,
			</if>
			<if test="bean.useTime != null">
				 use_time,
			</if>
			<if test="bean.status != null">
				 status,
			</if>
		 </trim>
		 <trim prefix="values (" suffix=")" suffixOverrides="," >
			<if test="bean.payCode!=null">
				 #{bean.payCode},
			</if>
			<if test="bean.amount!=null">
				 #{bean.amount},
			</if>
			<if test="bean.createTime!=null">
				 #{bean.createTime},
			</if>
			<if test="bean.useUserId!=null">
				 #{bean.useUserId},
			</if>
			<if test="bean.useTime!=null">
				 #{bean.useTime},
			</if>
			<if test="bean.status!=null">
				 #{bean.status},
			</if>
		 </trim>
	</insert>

	<!-- 插入或者更新 （匹配有值的字段）-->
	<insert id="insertOrUpdate" parameterType="com.easymusic.entity.po.PayCodeInfo">
		 INSERT INTO pay_code_info
		 <trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="bean.payCode != null">
				 pay_code,
			</if>
			<if test="bean.amount != null">
				 amount,
			</if>
			<if test="bean.createTime != null">
				 create_time,
			</if>
			<if test="bean.useUserId != null">
				 use_user_id,
			</if>
			<if test="bean.useTime != null">
				 use_time,
			</if>
			<if test="bean.status != null">
				 status,
			</if>
		 </trim>
		 <trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="bean.payCode!=null">
				 #{bean.payCode},
			</if>
			<if test="bean.amount!=null">
				 #{bean.amount},
			</if>
			<if test="bean.createTime!=null">
